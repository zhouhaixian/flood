{
  "name": "vite-vue-typescript-starter",
  "private": true,
  "version": "3.0.1",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e ./src/server/.env.development.local -- cross-env NODE_ENV=development nodemon -w src/server -x tsx src/server/main.ts",
