    "express": "^4.18.2",
    "highlight.js": "^11.9.0",
    "prisma": "^5.7.1",
    "puppeteer": "^21.6.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "tsx": "^4.4.0",
    "typescript": "^5.3.2",
    "user-agents": "^1.1.76",
    "vite-express": "*",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.3",
    "@vitejs/plugin-vue": "^4.5.0",
    "cross-env": "^7.0.3",
